initLuaScript: "scripts/init.lua"; #Initial lua script for GameInit
windowSize: 512 512; #Game resolution (if <= 10 is relative, if > 10 is fixed)
bufferSize: 512 512; # Game frame buffer size (if <= 10 is relative, if > 10 is fixed)
windowTitle: "Envari Engine";
vsync: 0; #0 disabled, 1 enabled
fpsLimit: 60; #FPS limit, disabled if value is less than 0
fpsFixed: 60; #Set fixed frames